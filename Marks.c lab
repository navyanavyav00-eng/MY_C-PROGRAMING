#include <stdio.h>

int main() {
    int marks;
    printf("Enter the marks: ");
    scanf("%d", &marks);

    if (marks >= 90) {
        printf("A grade");
    }
    else if (marks >= 75) {   // 75 to 89
        printf("B grade");
    }
    else if (marks >= 60) {   // 60 to 74
        printf("C grade");
    }
    else if (marks >= 50) {   // 50 to 59
        printf("D grade");
    }
    else {                    // below 50
        printf("F grade");
    }

    return 0;
}
